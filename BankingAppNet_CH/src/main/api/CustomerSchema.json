{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jsonschema.net",
  "type":"object",
  "properties": {
      "Customer": {
         "id": "http://jsonschema.net/1/customer",
          "type": "object",
          "properties": {
               "CustomerId": {
                  "id": "http://jsonschema.net/1/cust_id",
                  "type": "string"
               },
               "Demographics": {
                  "id": "http://jsonschema.net/1/payment/demographics",
                  "type": "object",
                  "properties": {
                       "FirstName": {
                        "id": "http://jsonschema.net/1/first_name",
                        "type": "string"
                       },
                       "LastName": {
                        "id": "http://jsonschema.net/1/last_name",
                        "type": "string"
                       },
                      "Address": {
                      "id": "http://jsonschema.net/1/address",
                      "type": "string"
                       },
                       "City": {
                      "id": "http://jsonschema.net/1/city",
                      "type": "string"
                       },
                      "Province": {
                      "id": "http://jsonschema.net/1/province",
                      "type": "string"
                       },
                      "Country": {
                      "id": "http://jsonschema.net/1/country",
                      "type": "string"
                       },
                       "Age": {
                      "id": "http://jsonschema.net/1/age",
                      "type": "integer"
                       },
                      "MaritalStatus": {
                      "id": "http://jsonschema.net/1/married",
                      "type": "string"
                       },
                       "email": {
                      "id": "http://jsonschema.net/1/email",
                      "type": "string"
                       },
                       "phone": {
                      "id": "http://jsonschema.net/1/phone",
                      "type": "string"
                       }
                  }
               },
                "Accounts":{
                  "id": "http://jsonschema.net/1/accounts",
                  "type": "array",
                  "items": {
                        "Account": {
                          "id": "http://jsonschema.net/1/accounts/1",
                          "type": "object",
                          "properties": {
                                "AccountNumber": {
                                "id": "http://jsonschema.net/1/accounts/1/account_num",
                                "type": "string"
                               },
                               "AccountType": {
                                "id": "http://jsonschema.net/1/accounts/1/account_type",
                                "type": "string"
                               },
                               "AccountBalance": {
                                "id": "http://jsonschema.net/1/accounts/1/account_balance",
                                "type": "number"
                               },
                               "AccountInterestRate": {
                                "id": "http://jsonschema.net/1/accounts/1/interest_rate",
                                "type": "number"
                               }
                          }
                        }
                  }
                
            },
            "Loans": {
              "id": "http://jsonschema.net/1/loans",
              "type": "object",
               "properties": {
                  "CreditCards": {
                    "id":"http://jsonschema.net/1/loans/cc",
                    "type": "array",
                     "items": {
                           "CreditCard": {
                            "id": "http://jsonschema.net/1/loans/cc/1",
                            "type": "object",
                            "properties": {
                               "CCType": {
                                "id": "http://jsonschema.net/1/accounts/1/cc_type",
                                "type": "string"
                                 },
                                 "CCNumber": {
                                "id": "http://jsonschema.net/1/accounts/1/cc_number",
                                "type": "string"
                                 },
                                 "CCBalance": {
                                "id": "http://jsonschema.net/1/accounts/1/cc_balance",
                                "type": "number"
                                 },
                                 "CCInterestRate": {
                                "id": "http://jsonschema.net/1/accounts/1/cc_interest_rate",
                                "type": "string"
                                 }
                                
                            }
                          }
                     }
                  },
                   "CreditLines":{
                    "id":"http://jsonschema.net/1/loans/cl",
                    "type": "array",
                     "items": {
                          "CreditLine": {
                            "id": "http://jsonschema.net/1/loans/cl/1",
                            "type": "object",
                            "properties": {
                                   "CLAcctNumber": {
                                    "id": "http://jsonschema.net/1/accounts/1/cl_acct_num",
                                    "type": "string"
                                   },  
                                   "CLLimit": {
                                    "id": "http://jsonschema.net/1/accounts/1/cl_limit",
                                    "type": "number"
                                   },  
                                   "CLInterestRate": {
                                    "id": "http://jsonschema.net/1/accounts/1/cl_interest_rate",
                                    "type": "number"
                                   },  
                                   "CLBalance": {
                                    "id": "http://jsonschema.net/1/accounts/1/cl_balance",
                                    "type": "number"
                                   }
                            
                            }
                          }
                     }
                   },
                   "Mortgages": {
                   "id":"http://jsonschema.net/1/loans/mortgages",
                   "type": "array",
                    "items": {
                         "Mortgage": {
                          "id": "http://jsonschema.net/1/loans/mortgages/1",
                          "type": "object",
                          "properties": {
                                 "MortgageAcctNumber": {
                                   "id": "http://jsonschema.net/1/accounts/1/mortgage_acct_num",
                                   "type": "string"
                                   },  
                                   "MortgageValue": {
                                    "id": "http://jsonschema.net/1/accounts/1/mortgage_value",
                                    "type": "number"
                                   },  
                                   "MortgagePaid": {
                                    "id": "http://jsonschema.net/1/accounts/1/mortgage_paid",
                                    "type": "number"
                                   },  
                                   "MortgageType": {
                                    "id": "http://jsonschema.net/1/accounts/1/mortgage_type",
                                    "type": "string"
                                   },  
                                   "MortgageEndPeriod": {
                                    "id": "http://jsonschema.net/1/accounts/1/mortgage_end_period",
                                    "type": "integer"
                                   },  
                                   "MortgageInterestRate": {
                                    "id": "http://jsonschema.net/1/accounts/1/mortgage_interest_rate",
                                    "type": "number"
                                   }
                          }
                       }
                    }
                   }
               }
            }
               
        }
        
    }
  }
}